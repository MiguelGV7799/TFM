[Title]
Sample input for radiation shielding

[Parameters]

 icntl   = 0
 maxcas  = 1e7
 maxbch  = 10

e-mode  = 2   
nucdata = 1     
dmax(2) = 20.0
lib(2)=20c


# Neutrones
emin(2) = 1.0e-6   # CAMBIO subir corte n térmico a 1 eV 

# Alfas 
#emin(18) = 1.0e-3  # DEFAULT 

# Electrones y positrones (EGS5)
#emin(12) = 1.0e-2  # CAMBIO: 10 keV para e- , EGS (100 keV)
#emin(13) = 1.0e-2  # CAMBIO: 10 keV para e+

# Fotones
#emin(14) = 1.0e-3  # DEFAULT 
negs    = 1        # CAMBIO: activar EGS5 para transporte EM detallado
ikerman = 1        # CAMBIO: prohibir KERMA para neutrones en t-deposit
ikermap = 1        # CAMBIO: prohibir KERMA para fotones en t-deposit

#controla la longitud de paso para partículas cargadas en ese material
chard(1)    = 1.0e-3   # CAMBIO: 1 μm para fuel (radio ~6.2 μm) 
chard(2) = 1.0e-3   
chard(3) = 1.0e-3 # CAMBIO: 1 μm para ablator 

kmout   = 1       # CAMBIO: escribir info de librerías nucleares usadas (nuclear data) en phits.out
icput   = 1       # CAMBIO: tiempos de CPU por proceso  útil para optimizar




